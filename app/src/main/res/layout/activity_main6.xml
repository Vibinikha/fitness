<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity6">

    <ImageSwitcher
        android:id="@+id/imageSwitcher"
        android:layout_width="309dp"
        android:layout_height="247dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="51dp"
        android:layout_marginTop="37dp"
        android:layout_marginEnd="51dp" />

    <ImageButton
        android:id="@+id/prevButton"
        android:layout_width="67dp"
        android:layout_height="55dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="26dp"
        android:layout_marginEnd="211dp"
        android:layout_marginBottom="374dp"
        android:layout_toStartOf="@+id/nextButton"
        android:background="#FF80AB"
        android:src="@drawable/baseline_arrow_back_ios_24"
        android:text="Left"
        tools:ignore="SpeakableTextPresentCheck"
        tools:src="@drawable/baseline_arrow_back_ios_24" />

    <ImageButton
        android:id="@+id/nextButton"
        android:layout_width="68dp"
        android:layout_height="48dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="39dp"
        android:layout_marginBottom="376dp"
        android:background="#FF80AB"
        android:src="@drawable/baseline_arrow_forward_ios_24"
        android:text="Right"
        tools:ignore="SpeakableTextPresentCheck"
        tools:src="@drawable/baseline_arrow_forward_ios_24" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="196dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="198dp"
        android:layout_marginBottom="311dp"
        android:text="Each Workout"
        android:textSize="30dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="194dp"
        android:layout_marginBottom="267dp"
        android:text="3 mins"
        android:textColor="#858080"
        android:textSize="30dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="364dp"
        android:layout_height="161dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="27dp"
        android:layout_marginBottom="91dp"
        android:text="Being physically active can improve your brain health, help manage weight, reduce the risk of disease, strengthen bones and muscles, and improve your ability to do everyday activities. Adults who sit less and do any amount of moderate-to-vigorous physical activity gain some health benefits.."
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textSize="19dp"
        android:textStyle="italic" />

    <Button
        android:id="@+id/button"
        style="@style/Widget.AppCompat.Button.Colored"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="144dp"
        android:layout_marginBottom="35dp"
        android:text="Start" />

</RelativeLayout>

